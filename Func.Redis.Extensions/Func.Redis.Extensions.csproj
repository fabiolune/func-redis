<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>func-redis-extensions</PackageId>
    <RepositoryUrl>https://github.com/fabiolune/func-redis.git</RepositoryUrl>
    <PackageTags>fp redis railway</PackageTags>
    <TargetFramework>net8</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageTags>fp redis railway</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Scrutor" Version="5.0.2" />
    <PackageReference Include="System.Text.Json" Version="8.0.5" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Func.Redis\Func.Redis.csproj" />
  </ItemGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>$(AssemblyName).Unit.Tests</_Parameter1>
    </AssemblyAttribute>
    <None Include="../README.md" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
